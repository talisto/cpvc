{
    "name": "talisto/cpvc",
    "type": "library",
    "description": "Composer Package Version Checker - checks your composer.json dependencies for newer versions",
    "keywords": [
        "composer",
        "composer.json",
        "version",
        "checker",
        "updates"
    ],
    "homepage": "https://github.com/talisto/cpvc",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Lyon",
            "email": "talisto@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Talisto\\Composer\\VersionCheck\\": "src/"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "composer/composer": "~1.4",
        "symfony/console": "~2.8",
        "doctrine/cache": "~1.5"
    },
    "require-dev": {
        "filp/whoops": "~1.1",
        "phpunit/phpunit": "~4.8"
    }
}