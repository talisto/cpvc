{
    "name": "talisto/cpvc",
    "type": "library",
    "description": "Composer Package Version Checker - checks your composer.json dependencies for newer versions",
    "keywords": [
        "composer",
        "composer.json",
        "version",
        "checker",
        "updates"
    ],
    "homepage": "https://github.com/talisto/cpvc",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Lyon",
            "email": "talisto@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "Talisto\\Composer\\VersionCheck\\": "src/"
        }
    },
    "require": {
        "composer/composer": "1.0.*@alpha",
        "symfony/console": "~2.5",
        "doctrine/cache": "~1.3"
    },
    "require-dev": {
        "filp/whoops": "~1.0",
        "phpunit/phpunit": "3.7.*"
    }
}